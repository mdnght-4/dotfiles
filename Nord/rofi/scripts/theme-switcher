#!/bin/sh
chosen=$(printf "Nord\nEverforest\nParadise\nGruvbox Material\nCatppuccin\nFrost\n" | rofi -dmenu -p "îˆ«" -i)

case "$chosen" in
  "Nord") nord ;;
  "Everforest") everforest ;;
  "Paradise") paradise ;;
  "Gruvbox Material") gruvbox-mat ;;
  "Catppuccin") catppuccin ;;
  "Frost") frost ;;
  *) exit 1 ;;
esac
